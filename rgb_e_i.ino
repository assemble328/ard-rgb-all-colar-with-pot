void setColor(int R, int G, int B) {
  analogWrite(PIN_RED,   R);
  analogWrite(PIN_GREEN, G);
  analogWrite(PIN_BLUE,  B);
  t = analogRead (A0);
   t=  map(t,0,1023,200,9000);
}
